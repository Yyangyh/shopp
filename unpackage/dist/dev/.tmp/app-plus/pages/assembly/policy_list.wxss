.button{
	width: 25%;
	height: 60rpx;
	line-height: 60rpx;
	margin-right: 40rpx;
	margin-bottom: 30rpx;
	text-align: center;
	font-size: 24rpx;
	color: #1D9DFF;
	border-radius: 20rpx;
	background: #e8f5ff;
}
.box:last-of-type{
	position: absolute;
	bottom: 20rpx;
	height: 80rpx;
}
.box:last-of-type button{
	border-radius: 50rpx;
}

