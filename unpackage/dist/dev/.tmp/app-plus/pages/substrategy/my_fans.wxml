<view class="content"><view class="status_bar"></view><returns vue-id="1" titles="{{title}}" bind:__l="__l"></returns><view class="top_box"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"><view class="{{['part',(show==1)?'active':'']}}">我的粉丝</view></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e"><view class="{{['part',(show==2)?'active':'']}}">我的关注</view></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" bindtap="__e"><view class="{{['part',(show==3)?'active':'']}}">我的访客</view></view></view><view class="list"><block wx:for="{{data}}" wx:for-item="item" wx:for-index="index"><view class="single_list"><view class="list_left"><image src="{{item.user.avatar}}" mode="widthFix"></image><view>{{item.user.user_name_view}}</view></view><block wx:if="{{item.is_follow===false}}"><view data-event-opts="{{[['tap',[['concern',['$0',index],[[['data','',index,'for_uid']]]]]]]}}" class="focus" bindtap="__e">关注</view></block><block wx:else><view data-event-opts="{{[['tap',[['concern',['$0',index],[[['data','',index,'for_uid']]]]]]]}}" class="focus" bindtap="__e">已关注</view></block></view></block></view></view>